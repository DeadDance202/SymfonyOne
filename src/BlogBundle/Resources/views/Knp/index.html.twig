{% extends 'base.html.twig' %}

    {% block body %}
<div id="wrapper">
  <div class="container">
     <div class="row" style="margin-top:50px;">


        {% for product in products %}
         <div class="col-md-4">
         <ul class="thumbnails" style="list-style: none;">
             <li>
            <a href="" class="thumbnail "><img class="img-rounded img-responsive " src="{{ product.image }}" alt="" style="height:250px"/>
                <h3>{{ product.name}}</h3></a>


                 </li>
             </ul>
            </div>
        {% endfor %}

     </div>
  </div>
</div>
    {% endblock %}